         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button layoutX="505.0" layoutY="332.0" mnemonicParsing="false" onAction="#ok" text="OK" />
      <Button layoutX="413.0" layoutY="332.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
